<div class="row">
  <div class="col s12">
    <h1 class="center-align">Students/index page</h1>
  </div>
</div>

<div class="row">
  <% @students.each do |student| %>
    <div class="col s6 m6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">Name: <%= student.name %></span>
          <p>Email: <%= student.email %></p>
        </div>
        <div class="card-action">
          <h3>Current enrollments</h3>
        </div>
      </div>
    </div>
  <% end %>
</div>